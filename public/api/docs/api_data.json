[  {    "type": "get",    "url": "/:topic/:from",    "title": "Read events from this topic's log, starting from the given time",    "version": "1.0.0",    "name": "Index",    "group": "Events",    "description": "<p>Read log from any valid HomeStream v1 topic These follow the format of hs/1/:namespace:object_id/:attribute or hs/1/:namespace:object_id/to/:to_namespace/:to_object_id/:to_attribute</p> <p>For example, to see historical occupancy changes in room 2 since 1589686339565, you would view /api/1/hs/1/com.hiome/room_2/occupancy/1589686339565</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "topic",            "description": "<p>Any valid HomeStream topic. Replace + wildcard with '~'</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": false,            "field": "from",            "description": "<p>timestamp (in milliseconds since epoch) to start reading from</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": true,            "field": "until",            "defaultValue": "now",            "description": "<p>timestamp (in milliseconds since epoch) to stop reading, defaults to current time</p>"          },          {            "group": "Parameter",            "type": "Number",            "optional": true,            "field": "limit",            "defaultValue": "1000",            "description": "<p>max number of events to return, even if we didn't reach &quot;until&quot; timestamp</p>"          },          {            "group": "Parameter",            "type": "Boolean",            "optional": true,            "field": "reverse",            "defaultValue": "false",            "description": "<p>if &quot;true&quot;, events will be returned in reverse chronological order</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "ts",            "description": "<p>timestamp of the event, as milliseconds since epoch</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "topic",            "description": "<p>topic where event was published</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "namespace",            "description": "<p>namespace of event</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "object_id",            "description": "<p>object that published this event</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "attribute",            "description": "<p>the attribute of the event</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "payload",            "description": "<p>full json of the payload, as a string</p>"          },          {            "group": "Success 200",            "type": "Boolean",            "optional": false,            "field": "retain",            "description": "<p>true if this event was a state change and retained</p>"          },          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "context_ts",            "description": "<p>if this event occurred in resposne to another, ts of the parent event</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "context_topic",            "description": "<p>if this event occurred in resposne to another, topic of the parent event</p>"          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "{\n  \"ts\": 1556767182,\n  \"topic\":\"hs/1/com.hiome/room_1578349369/occupancy\",\n  \"namespace\":\"com.hiome\",\n  \"object_id\":\"room_1578349369\",\n  \"attribute\":\"occupancy\",\n  \"payload\":\"{'val':0,'ts':1556767182}\",\n  \"retain\":true\n}",          "type": "json"        }      ]    },    "filename": "api/1/events.js",    "groupTitle": "Events",    "sampleRequest": [      {        "url": "http://hiome.local/api/1/:topic/:from"      }    ]  },  {    "type": "get",    "url": "/:topic/retained",    "title": "Read latest retained state of topic(s)",    "version": "1.0.0",    "name": "Retained",    "group": "Events",    "description": "<p>This will return at most 1 event per matching topic, which is that topic's most recent retained state.</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "topic",            "description": "<p>Any valid HomeStream topic. Replace + wildcard with '~'</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Number",            "optional": false,            "field": "ts",            "description": "<p>timestamp of the event, as milliseconds since epoch</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "topic",            "description": "<p>topic where event was published</p>"          },          {            "group": "Succes